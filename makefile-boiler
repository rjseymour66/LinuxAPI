objects = new_free_and_sbrk.o ../lib/error_functions.o ../lib/get_num.o

new_free_and_sbrk : $(objects)
	gcc -o new_free_and_sbrk $(objects)

new_free_and_sbrk.o : new_free_and_sbrk.c ../lib/tlpi_hdr.h
	gcc -I ../lib -c new_free_and_sbrk.c

.PHONY : clean
clean : 
	-rm new_free_and_sbrk new_free_and_sbrk.o
